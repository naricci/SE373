mixin generateTable(numbers)
  -const color = ((1<<24)*Math.random()|0).toString(16)
  table(border="1")
    tbody
      each num in numbers
        tr
        each num in numbers
          td(style="background-color: #{color};")= color
            br
              span(style="color=fff;")= color
      
//mixin displayNums(numbers)
